import turtle
myPen = turtle.Turtle()

myPen.penup()
myPen.setpos(0,0)
myPen.pendown()
myPen.circle(20)

import turtle
myPen = turtle.Turtle()

myPen.penup()
myPen.setpos(0,60)
myPen.pendown()
myPen.circle(20)
myPen.up()


